msgid "Select your preferred notification track"
msgstr "希望する通知トラックを選択してください"

#. audinary@zener-diode->settings-schema.json->custom-track->tooltip
msgid "Select the notification track for hourly alarm"
msgstr "通知トラックを1時間ごとに選択します"

#. audinary@zener-diode->settings-schema.json->font->description
msgid "Select a font"
msgstr "フォントを選択"

#. audinary@zener-diode->settings-schema.json->font-size->description
msgid "Font size"
msgstr "フォントサイズ"

#. audinary@zener-diode->settings-schema.json->font-size->tooltip
msgid "Increase or decrease the note font size."
msgstr "音符のフォントサイズを増減します。"

#. audinary@zener-diode->settings-schema.json->font-bold->description
msgid "Make text bold"
